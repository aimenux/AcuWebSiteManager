	<Include  table="BAccount"
		  condition ="where BAccountID in 
					(select BAccountID from Vendor where CompanyID = BAccount.CompanyID and TaxAgency = 1)
					or Type = 'CP' or Type = 'OR' or Type = 'OB' or Type = 'EP'" />
	<Include  table="Contract"
			  condition ="where BaseType = 'T' or BaseType='R'" />
	<Include  table="PMMarkup"
			  condition ="where ProjectID in 
						(select ContractID from Contract where CompanyID = PMMarkup.CompanyID and (NonProject = 1 or BaseType = 'T') )"/>
	<Include  table="ContractBillingSchedule"
			  condition ="where ContractID in 
						(select ContractID from Contract where CompanyID = ContractBillingSchedule.CompanyID and BaseType = 'T')">
	</Include>
	<Include  table="PMRetainageStep"
			  condition ="where ProjectID in 
						(select ContractID from Contract where BaseType = 'T')"/>
	<Include  table="ContractDetail" condition="where ContractID in (select ContractID from Contract where CompanyID = ContractDetail.CompanyID and BaseType = 'T')" />
	<Include  table="FinPeriod">
		 <ResetColumn name="Active" value="False" />
		 <ResetColumn name="Status" value="Inactive" />
		 <ResetColumn name="Closed" value="False" />
		 <ResetColumn name="APClosed" value="False" />
		 <ResetColumn name="ARClosed" value="False" />
		 <ResetColumn name="INClosed" value="False" />
		 <ResetColumn name="CAClosed" value="False" />
		 <ResetColumn name="FAClosed" value="False" />
		 <ResetColumn name="DateLocked" value="False" />
	</Include>
	<Include table="TaxPeriod">
		<ResetColumn name="Status" value="N" />
	</Include>
	<Include  table="PMTask" condition ="where (ProjectID in (select ContractID from Contract where CompanyID = PMTask.CompanyID and BaseType = 'R') or ProjectID = 0)" />
	<Include  table="PMAllocationDetail">
		<ResetColumn name="ProjectID" value="NULL" />
	</Include>
	<Include  table="Vendor" condition ="where BAccountID in (select BAccountID from EPEmployee where CompanyID = Vendor.CompanyID) or TaxAgency = 1"/>
	<Include  table="Contact" condition="where ContactID in (select ContactID from INSite where CompanyID = Contact.CompanyID)
						or ContactID in (select ContactID from Contact where CompanyID = Contact.CompanyID and BAccountID in (select BAccountID from Vendor where CompanyID = Contact.CompanyID and TaxAgency = 1))
						or ContactID in (select ContactID from Contact where CompanyID = Contact.CompanyID and ContactType = 'EP')
						or ContactID in (select ContactID from Contact where CompanyID = Contact.CompanyID and BAccountID in (select BAccountID from Branch where CompanyID = Contact.CompanyID))" />

	<Include  table="Address" condition="where AddressID in (select AddressID from INSite where CompanyID = Address.CompanyID)
						or AddressID in (select AddressID from Address where CompanyID = Address.CompanyID and BAccountID in (select BAccountID from Vendor where CompanyID = Address.CompanyID and TaxAgency = 1))
						or AddressID in (select AddressID from Address where CompanyID = Address.CompanyID and BAccountID in (select BAccountID from Company where CompanyID = Address.CompanyID))
						or AddressID in (select AddressID from Address where CompanyID = Address.CompanyID and BAccountID in (select BAccountID from Branch where CompanyID = Address.CompanyID))" />
	<Include table="Location" condition="where BAccountID in
						(select BAccountID from BAccount where CompanyID = Location.CompanyID and (BAccountID in 
