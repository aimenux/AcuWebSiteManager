	<Include  table="CSAnswers"
				condition ="where RefNoteID in (select NoteID from InventoryItem where CompanyID=CSAnswers.CompanyID)
				or RefNoteID in (select NoteID from Contract where CompanyID=CSAnswers.CompanyID AND BaseType='P') 
				or RefNoteID in (select NoteID from PMTask where CompanyID=CSAnswers.CompanyID) 
				or RefNoteID in (select NoteID from PMAccountGroup where CompanyID=CSAnswers.CompanyID)
				or RefNoteID in (select NoteID From BAccount where CompanyID=CSAnswers.CompanyID AND (BAccountID in 
					(select BAccountID from Vendor where CompanyID=CSAnswers.CompanyID AND TaxAgency = 1) or Type = 'CP' or Type = 'OR' or Type = 'OB' or Type = 'EP')) 
				or RefNoteID in (select NoteID from BAccount where CompanyID=CSAnswers.CompanyID AND BAccountID in (
					Select BAccountID From Vendor where CompanyID=CSAnswers.CompanyID AND (BAccountID in (
						select BAccountID from EPEmployee where CompanyID=CSAnswers.CompanyID) or TaxAgency = 1)))
				or RefNoteID in (select NoteID From Contact where CompanyID=CSAnswers.CompanyID AND (ContactID in (select ContactID from INSite where CompanyID=CSAnswers.CompanyID)
					or ContactID in (select ContactID from Contact where CompanyID=CSAnswers.CompanyID AND BAccountID in (select BAccountID from Vendor where CompanyID=CSAnswers.CompanyID AND TaxAgency = 1))
					or ContactID in (select ContactID from Contact where CompanyID=CSAnswers.CompanyID AND ContactType = 'EP')
					or ContactID in (select ContactID from Contact where CompanyID=CSAnswers.CompanyID AND BAccountID in (select BAccountID from Branch where CompanyID=CSAnswers.CompanyID))))">
	</Include>
