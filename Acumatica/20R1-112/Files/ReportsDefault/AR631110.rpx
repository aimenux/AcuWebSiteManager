              <Name>BaseAgeTot00</Name>
              <ValueExpr>=$BaseAgeTot00+$BaseAgeBal00</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BaseAgeTot01</Name>
              <ValueExpr>=$BaseAgeTot01+$BaseAgeBal01</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BaseAgeTot02</Name>
