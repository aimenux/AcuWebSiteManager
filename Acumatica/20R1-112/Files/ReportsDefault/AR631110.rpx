      <Headers>
        <Header Name="groupHeaderSection2">
          <GridSize>8px, 8px</GridSize>
          <Height>14px</Height>
          <Variables>
            <ReportVariable>
              <Name>DocBal</Name>
              <ValueExpr>=[ARRegisterReport.SignBalance] * [ARRegisterReport.CuryOrigDocAmt] + [ARAdjustedBalanceAtDate.CuryLineTotal] - [ARAdjustingBalanceAtDate.CuryLineTotal]</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BaseDocBal</Name>
              <ValueExpr>=[ARRegisterReport.SignBalance] * [ARRegisterReport.OrigDocAmt] + [ARAdjustedBalanceAtDate.LineTotal] - [ARAdjustingBalanceAtDate.LineTotal]</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>PrintDoc</Name>
              <ValueExpr>=($DocBal &lt;&gt; 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>DueDate</Name>
              <ValueExpr>= Payments.GetDocumentDateForAgedReport(#010;	@AgeDate,#010;	[ARInvoice.DueDate],#010;	[ARRegisterReport.DocDate],#010;	[ARStatementCycle.AgeBasedOn],#010;	[ARSetup.AgeCredits])</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BucketNumber</Name>
              <ValueExpr>= Payments.GetBucketNumberForAgedReport(#010;	@AgeDate,#010;	$DueDate,#010;	0,#010;	CInt([ARStatementCycle.AgeDays00]),#010;	CInt([ARStatementCycle.AgeDays01]),#010;	CInt([ARStatementCycle.AgeDays02]),#010;	[ARStatementCycle.UseFinPeriodForAging],#010;	False)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>CurrBal</Name>
              <ValueExpr>= IIF($BucketNumber = 0, $DocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>AgeBal00</Name>
              <ValueExpr>= IIF($BucketNumber = 1, $DocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>AgeBal01</Name>
              <ValueExpr>= IIF($BucketNumber = 2, $DocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>AgeBal02</Name>
              <ValueExpr>= IIF($BucketNumber = 3, $DocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>AgeBal03</Name>
              <ValueExpr>= IIF($BucketNumber = 4, $DocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>CurrTot</Name>
              <ValueExpr>=$CurrTot+$CurrBal</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>AgeTot00</Name>
              <ValueExpr>=$AgeTot00+$AgeBal00</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>AgeTot01</Name>
              <ValueExpr>=$AgeTot01+$AgeBal01</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>AgeTot02</Name>
              <ValueExpr>=$AgeTot02+$AgeBal02</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>AgeTot03</Name>
              <ValueExpr>=$AgeTot03+$AgeBal03</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BalTot</Name>
              <ValueExpr>=$BalTot+$DocBal</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BaseCurrBal</Name>
              <ValueExpr>=IIF($BucketNumber = 0, $BaseDocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BaseAgeBal00</Name>
              <ValueExpr>=IIF($BucketNumber = 1, $BaseDocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BaseAgeBal01</Name>
              <ValueExpr>=IIF($BucketNumber = 2, $BaseDocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BaseAgeBal02</Name>
              <ValueExpr>=IIF($BucketNumber = 3, $BaseDocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BaseAgeBal03</Name>
              <ValueExpr>=IIF($BucketNumber = 4, $BaseDocBal, 0)</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>BaseCurrTot</Name>
              <ValueExpr>=$BaseCurrTot+$BaseCurrBal</ValueExpr>
            </ReportVariable>
