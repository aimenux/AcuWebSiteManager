<?xml version="1.0" encoding="utf-8"?>
<Report Name="report1">
  <ExportFileName>=Format( 'ARInvoice {0} {1}', IsNull([@DocType],''),IsNull([@RefNbr],''))</ExportFileName>
  <Filters>
    <FilterExp>
      <DataField>ARInvoice.DocType</DataField>
      <Value>@DocType</Value>
    </FilterExp>
    <FilterExp>
      <DataField>ARInvoice.RefNbr</DataField>
      <Value>@RefNbr</Value>
    </FilterExp>
  </Filters>
  <MailSettings>
    <ActivitySource>ARInvoice.NoteID</ActivitySource>
    <Body>='Dear Customer,#010;#010;This is an automatic email to notify you that you have a new Invoice available on your account.  You will find a copy of'+' '+[ARInvoice.PrintDocType]+' '+'#'+[ARInvoice.RefNbr]+ IIf( [ARInvoice.InvoiceNbr] &lt;&gt; Null, ' '+'(Customer Ref. Nbr.'+' '+[ARInvoice.InvoiceNbr]+')', '' )+' '+'attached to this email.  #010;#010;If you have any questions, please don’t hesitate to contact us at email.  #010;#010;Thank you for choosing'+' '+[CompanyContact.FullName]+'.'</Body>
    <Format></Format>
    <ParentSource>Customer.NoteID</ParentSource>
    <Subject>=[CompanyContact.FullName]+':'+' '+[ARInvoice.PrintDocType]+' '+'#'+[ARInvoice.RefNbr]+ IIf( [ARInvoice.InvoiceNbr] &lt;&gt; Null, ' '+'(Customer Ref. Nbr.'+' '+[ARInvoice.InvoiceNbr]+')', '' ) +' '+'is now available'</Subject>
    <TemplateID></TemplateID>
    <To>=IsNull([BillingContact.Email],'')</To>
  </MailSettings>
  <Parameters>
    <ReportParameter>
      <ColumnSpan>2</ColumnSpan>
      <DefaultValue>INV</DefaultValue>
      <Name>DocType</Name>
      <Nullable>True</Nullable>
      <Prompt>Document Type</Prompt>
      <ValidValues>
        <ParameterValue>
          <Label>Invoice</Label>
          <Value>INV</Value>
        </ParameterValue>
        <ParameterValue>
