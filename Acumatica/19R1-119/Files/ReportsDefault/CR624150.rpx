        </ReportVariable>
        <ReportVariable>
          <Name>WonQtySummary</Name>
          <ValueExpr>=IIf([CROpportunity.MajorStatus]=4 And [CROpportunity.Status]='W'#010;And #010;([CROpportunity.CloseDate] &gt; [@StartDate]#010;And #010;[CROpportunity.CloseDate] &lt; [@EndDate]),$WonQtySummary+1,$WonQtySummary)</ValueExpr>
