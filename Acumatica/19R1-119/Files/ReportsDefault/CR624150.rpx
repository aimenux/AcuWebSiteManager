        </ReportVariable>
        <ReportVariable>
          <Name>AvgTime</Name>
          <ResetGroup>group1</ResetGroup>
          <ValueExpr>=IIf($Time &gt; 0, CInt($Time/(IIf(($WonQty + $LostQty) &gt; 0, ($WonQty + $LostQty), 1))), 0)</ValueExpr>
