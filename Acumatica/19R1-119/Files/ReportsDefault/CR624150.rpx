          <ResetGroup>group1</ResetGroup>
          <ValueExpr>=IIf([CROpportunity.MajorStatus]=4 And [CROpportunity.Status]='W' #010;And#010;([CROpportunity.CloseDate] &gt; [@StartDate]#010;And #010;[CROpportunity.CloseDate] &lt; [@EndDate]),$WonQty+1,$WonQty)</ValueExpr>
