    <Group Name="groupAdjustedDocument">
      <Description>Grouping by Adjusted Document</Description>
      <Grouping>
        <GroupExp>
          <DataField>=[ARStatementAdjust.AdjdRefNbr]</DataField>
        </GroupExp>
        <GroupExp>
          <DataField>=[ARStatementAdjust.AdjdDocType]</DataField>
        </GroupExp>
      </Grouping>
      <Headers>
        <Header Name="groupHeaderSection_Application">
          <GridSize>4px, 4px</GridSize>
          <Height>0.48328cm</Height>
          <ProcessOrder>Always</ProcessOrder>
          <Variables>
            <ReportVariable>
              <Name>isOrphanApplication</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>=[ARStatementDetailInfo.RefNbr] = NULL</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>foreignBranchDescription</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= IIF(#010;	[ARStatementAdjust.IsIncomingApplication],#010;	[ARStatementAdjust.AdjgBranchID],#010;	[ARStatementAdjust.AdjdBranchID])</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>foreignCustomerDescription</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= IIF(#010;	[ARStatementAdjust.IsInterCustomerApplication],#010;	[AdjustingCustomer.AcctCD],#010;	[AdjustedCustomer.AcctCD])</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>totalGainLoss</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= 0 /* because the value is already contained in CuryAdjdAmt */</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>totalWriteOff</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= IIF(#010;	[ARStatementAdjust.IsIncomingApplication],#010;	Sum([ARStatementAdjust.CuryAdjdWOAmt]),#010;	Sum([ARStatementAdjust.CuryAdjgWOAmt]))</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>totalCashDiscount</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= IIF(#010;	[ARStatementAdjust.IsIncomingApplication],#010;	Sum([ARStatementAdjust.CuryAdjdDiscAmt]),#010;	Sum([ARStatementAdjust.CuryAdjgDiscAmt]))</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>totalApplication</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= IIF(#010;	[ARStatementAdjust.IsIncomingApplication],#010;	Sum([ARStatementAdjust.CuryAdjdAmt]),#010;	Sum([ARStatementAdjust.CuryAdjgAmt]))</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>totalBalanceDelta</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= [ARStatementAdjust.SignBalanceDelta] * (#010;	IIF(#010;		[ARStatementAdjust.IsIncomingApplication],#010;		$totalGainLoss + $totalWriteOff + $totalCashDiscount,#010;		0)#010;	+ IIF(#010;		([ARStatementAdjust.IsInterStatementApplication] = True)#010;			OR ([ARStatementAdjust.IsSelfVoidingVoidApplication] = True), #010;		$totalApplication, #010;		0))</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>amountDescription</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= Payments.GetAmountDescriptionForStatementApplication(#010;	CDec($totalWriteOff), #010;	CDec($totalCashDiscount), #010;	CDec($totalGainLoss), #010;	[ARStatement.LocaleName])</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>adjustingDescription</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= Report.ExtToUI('ARStatementAdjust.AdjgDocType', [ARStatementAdjust.AdjgDocType])#010;	+ ' ' #010;	+ [ARStatementAdjust.AdjgRefNbr]#010;	+ IIF(#010;		[ARStatementAdjust.IsInterCurrencyApplication]#010;			AND [ARStatementAdjust.AdjgCuryID] &lt;&gt; [ARStatement.CuryID], #010;		' (' + [ARStatementAdjust.AdjgCuryID] + ')',#010;		'')#010;	+ IIF(#010;		[ARStatementAdjust.IsInterCustomerApplication]#010;			AND [ARStatementAdjust.AdjgCustomerID] &lt;&gt; [ARStatement.CustomerID],#010;		' (' + [AdjustingCustomer.AcctCD] + ')',#010;		'')#010;	+ IIF(#010;		[ARStatementAdjust.IsInterBranchApplication]#010;			AND [ARStatementAdjust.AdjgBranchID] &lt;&gt; [ARStatement.BranchID],#010;		' by ' + [ARStatementAdjust.AdjgBranchID],#010;		'')</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>adjustedDescription</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= Report.ExtToUI('ARStatementAdjust.AdjdDocType', [ARStatementAdjust.AdjdDocType])#010;	+ ' ' #010;	+ [ARStatementAdjust.AdjdRefNbr]#010;	+ IIF(#010;		[ARStatementAdjust.IsInterCurrencyApplication]#010;			AND [ARStatementAdjust.AdjdCuryID] &lt;&gt; [ARStatement.CuryID], #010;		' (' + [ARStatementAdjust.AdjdCuryID] + ')',#010;		'')#010;	+ IIF(#010;		[ARStatementAdjust.IsInterCustomerApplication]#010;			AND [ARStatementAdjust.AdjdCustomerID] &lt;&gt; [ARStatement.CustomerID],#010;		' (' + [AdjustedCustomer.AcctCD] + ')',#010;		'')#010;	+ IIF(#010;		[ARStatementAdjust.IsInterBranchApplication]#010;			AND [ARStatementAdjust.AdjdBranchID] &lt;&gt; [ARStatement.BranchID],#010;		' by ' + [ARStatementAdjust.AdjdBranchID],#010;		'')</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>applicationDescription</Name>
              <ResetGroup>groupAdjustedDocument</ResetGroup>
              <ValueExpr>= ''#010;	+ IIF(#010;		NOT([ARStatementAdjust.IsInterStatementApplication]), #010;		$amountDescription + ' ',#010;		'')#010;	+ IIF(#010;		([ARStatementAdjust.IsInterStatementApplication] = True)#010;			OR ([ARStatementAdjust.IsSelfVoidingVoidApplication] = True)#010;			OR $isOrphanApplication, #010;		$adjustingDescription#010;			+ ' '#010;			+ Payments.AppliedToLocalized([ARStatement.LocaleName])#010;			+ ' ', #010;		'')#010;	+ $adjustedDescription</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>LineBalance</Name>
              <ProcessOrder>WhilePrint</ProcessOrder>
              <ValueExpr>= $LineBalance + $totalBalanceDelta</ValueExpr>
            </ReportVariable>
            <ReportVariable>
              <Name>ConsolLineBalance</Name>
              <ProcessOrder>WhilePrint</ProcessOrder>
              <ValueExpr>= $ConsolLineBalance + $totalBalanceDelta</ValueExpr>
            </ReportVariable>
          </Variables>
          <VisibleExpr>= [ARStatementAdjust.AdjgRefNbr] &lt;&gt; NULL #010;	AND $totalBalanceDelta &lt;&gt; 0</VisibleExpr>
          <Items>
            <TextBox Name="textBox58_a75">
              <Format>=[ARStatementDetailInfo.CuryOrigDocAmt.Format]</Format>
              <Location>592px, 0px</Location>
              <ProcessOrder>WhilePrint</ProcessOrder>
              <Size>104px, 16px</Size>
              <Style>
                <TextAlign>Right</TextAlign>
              </Style>
              <StyleName>Normal</StyleName>
              <Value>=$LineBalance + $CuryBegBalance</Value>
              <VisibleExpr>=IIF([ARSetup.ConsolidatedStatement]=false,True,False)</VisibleExpr>
            </TextBox>
            <TextBox Name="textBox59_9a9">
              <CanGrow>True</CanGrow>
              <Location>72px, 0px</Location>
              <Size>416px, 16px</Size>
              <StyleName>Normal</StyleName>
              <Value>=$applicationDescription</Value>
            </TextBox>
            <TextBox Name="textBox60">
              <Format>=[ARStatementDetailInfo.OrigDocAmt.Format]</Format>
              <Location>488px, 0px</Location>
              <ProcessOrder>WhilePrint</ProcessOrder>
              <Size>104px, 16px</Size>
              <Style>
                <TextAlign>Right</TextAlign>
                <VerticalAlign>Top</VerticalAlign>
              </Style>
              <StyleName>Normal</StyleName>
              <Value>=$totalBalanceDelta</Value>
            </TextBox>
            <TextBox Name="textBox61">
              <Format>=[ARStatementDetailInfo.CuryOrigDocAmt.Format]</Format>
              <Location>592px, 0px</Location>
              <ProcessOrder>WhilePrint</ProcessOrder>
              <Size>104px, 16px</Size>
              <Style>
                <TextAlign>Right</TextAlign>
              </Style>
              <StyleName>Normal</StyleName>
              <Value>=$ConsolLineBalance + $CustBegBalance</Value>
              <VisibleExpr>=IIF([ARSetup.ConsolidatedStatement]=True,True,False)</VisibleExpr>
            </TextBox>
          </Items>
        </Header>
      </Headers>
      <Footers>
        <Footer Name="groupFooterSection2">
          <Height>0px</Height>
          <Visible>False</Visible>
        </Footer>
      </Footers>
    </Group>
